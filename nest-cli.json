{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/tarea_project/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/tarea_project/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/tarea_project",
  "projects": {
    "tarea_project": {
      "type": "application",
      "root": "apps/tarea_project",
      "entryFile": "main",
      "sourceRoot": "apps/tarea_project/src",
      "compilerOptions": {
        "tsConfigPath": "apps/tarea_project/tsconfig.app.json"
      }
    },
    "cris_app": {
      "type": "application",
      "root": "apps/cris_app",
      "entryFile": "main",
      "sourceRoot": "apps/cris_app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cris_app/tsconfig.app.json"
      }
    }
  }
}